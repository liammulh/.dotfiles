alias ..="cd .."
alias c="clear"
alias l="ls -ahl"
alias q="exit"
alias v="vim"
alias python="python3"

# Command line prompt.
PS1="> "

# No bells.
bind 'set bell-style none'

# Set default editor.
export EDITOR=

# Put path stuff here.
export PATH=$PATH:/opt/homebrew/bin:$HOME/.cargo/bin:$HOME/.local/bin:
